<hibernate-mapping>
	<class name = "model.inspection.Inspection" table = "INSPECTION">
		<id name = "inspectionID" column = "INSPECTION_ID">
			<generator class = "assigned"></generator>
		</id>
		
		<property name = "inspectionType" Column = "INSPECTIONTYPE" />
		<property name = "inspectionDate" Column = "INSPECTIONDATE" />
		<property name = "inspector" Column = "INSPECTOR" />
		<property name = "outcome" Column = "OUTCOME" />
	</class>
</hibernate-mapping>
